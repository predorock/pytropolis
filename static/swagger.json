{
    "swagger": "2.0",
    "info": {
        "version": "0.0.1",
        "title": "Pytropolis"
    },
    "paths": {
        "/health": {
            "get": {
                "summary": "Health Check",
                "description": "",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        },
        "/api/runner/start": {
            "post": {
                "summary": "Execute a script",
                "description": "",
                "responses": {
                    "200": {
                        "description": "Success",
                        "schema": {
                            "$ref": "#/definitions/execution_result"
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                },
                "parameters": [
                    {
                        "name": "script",
                        "in": "formData",
                        "type": "file",
                        "required": true,
                        "description": "The Python script to execute."
                    },
                    {
                        "name": "requirements",
                        "in": "formData",
                        "type": "file",
                        "required": true,
                        "description": "The requirements file for the script."
                    },
                    {
                        "name": "venv_name",
                        "in": "formData",
                        "type": "string",
                        "required": false,
                        "default": "default",
                        "description": "The name of the virtual environment to use."
                    },
                    {
                        "name": "script_name",
                        "in": "formData",
                        "type": "string",
                        "required": false,
                        "default": "algo",
                        "description": "The name of the script."
                    },
                    {
                        "name": "env_params",
                        "in": "formData",
                        "type": "JSON",
                        "required": false,
                        "default": {},
                        "description": "The environment parameters to use."
                    },
                    {
                        "name": "command",
                        "in": "formData",
                        "type": "string",
                        "required": false,
                        "default": "same as script_name",
                        "description": "The command customisation to use."
                    }
                ],
                "tags": [
                    "Runner API"
                ],
                "consumes": [
                    "multipart/form-data"
                ]
            }
        },
        "/api/runner/enqueue": {
            "post": {
                "summary": "Enqueue a script",
                "description": "",
                "responses": {
                    "200": {
                        "description": "Success",
                        "schema": {
                            "$ref": "#/definitions/execution_result"
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                },
                "parameters": [
                    {
                        "name": "script",
                        "in": "formData",
                        "type": "file",
                        "required": true,
                        "description": "The Python script to execute."
                    },
                    {
                        "name": "requirements",
                        "in": "formData",
                        "type": "file",
                        "required": true,
                        "description": "The requirements file for the script."
                    },
                    {
                        "name": "venv_name",
                        "in": "formData",
                        "type": "string",
                        "required": false,
                        "default": "default",
                        "description": "The name of the virtual environment to use."
                    },
                    {
                        "name": "script_name",
                        "in": "formData",
                        "type": "string",
                        "required": false,
                        "default": "algo",
                        "description": "The name of the script."
                    },
                    {
                        "name": "env_params",
                        "in": "formData",
                        "type": "JSON",
                        "required": false,
                        "default": {},
                        "description": "The environment parameters to use."
                    },
                    {
                        "name": "command",
                        "in": "formData",
                        "type": "string",
                        "required": false,
                        "default": "same as script name",
                        "description": "The command customisation to use."
                    }
                ],
                "tags": [
                    "Runner API"
                ],
                "consumes": [
                    "multipart/form-data"
                ]
            }
        },
        "/api/venv": {
            "get": {
                "summary": "Get the list of virtual environments",
                "description": "",
                "responses": {
                    "200": {
                        "description": "Success",
                        "schema": {
                            "$ref": "#/definitions/venv_list"
                        }
                    }
                },
                "tags": [
                    "Virtual Environment API"
                ]
            },
            "post": {
                "summary": "Create a new virtual environment",
                "description": "",
                "responses": {
                    "200": {
                        "description": "Success",
                        "schema": {
                            "$ref": "#/definitions/venv_creation_result"
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                },
                "parameters": [
                    {
                        "name": "venv_name",
                        "in": "formData",
                        "type": "string",
                        "required": true,
                        "description": "The name of the virtual environment to create."
                    },
                    {
                        "name": "requirements_file",
                        "in": "formData",
                        "type": "file",
                        "required": false,
                        "description": "The requirements file for the virtual environment."
                    }
                ],
                "tags": [
                    "Virtual Environment API"
                ],
                "consumes": [
                    "multipart/form-data"
                ]
            },
            "put": {
                "summary": "Update a virtual environment with a requirements file",
                "description": "",
                "responses": {
                    "200": {
                        "description": "Success",
                        "schema": {
                            "$ref": "#/definitions/venv_creation_result"
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    }
                },
                "parameters": [
                    {
                        "name": "venv_name",
                        "in": "formData",
                        "type": "string",
                        "required": true,
                        "description": "The name of the virtual environment to create."
                    },
                    {
                        "name": "requirements_file",
                        "in": "formData",
                        "type": "file",
                        "required": false,
                        "description": "The requirements file for the virtual environment."
                    }
                ],
                "tags": [
                    "Virtual Environment API"
                ],
                "consumes": [
                    "multipart/form-data"
                ]
            },
            "delete": {
                "summary": "Delete a virtual environment",
                "description": "",
                "responses": {
                    "200": {
                        "description": "Success",
                        "schema": {
                            "$ref": "#/definitions/venv_deletion_result"
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "venv_name",
                        "in": "formData",
                        "type": "string",
                        "required": true,
                        "description": "The name of the virtual environment to delete."
                    }
                ],
                "tags": [
                    "Virtual Environment API"
                ],
                "consumes": [
                    "multipart/form-data"
                ]
            }
        }
    },
    "definitions": {
        "execution_result": {
            "properties": {
                "output_log": {
                    "type": "string",
                    "description": "The path to the log file containing the script's output."
                },
                "venv_name": {
                    "type": "string",
                    "description": "The name of the virtual environment used."
                },
                "result": {
                    "type": "string",
                    "description": "The result of the script execution ('success' or 'error')."
                },
                "message": {
                    "type": "string",
                    "description": "A message describing the result of the script execution."
                }
            }
        },
        "venv_list": {
            "properties": {
                "venv_list": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "description": "The name of a virtual environment."
                    }
                }
            }
        },
        "venv_creation_result": {
            "properties": {
                "message": {
                    "type": "string",
                    "description": "A message describing the result of the virtual environment creation."
                },
                "venv_path": {
                    "type": "string",
                    "description": "The path to the virtual environment."
                }
            }
        },
        "venv_deletion_result": {
            "properties": {
                "message": {
                    "type": "string",
                    "description": "A message describing the result of the virtual environment deletion."
                }
            }
        }
    }
}